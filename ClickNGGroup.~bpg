#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = clickng.exe Resource_kompilieren.bat
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

clickng.exe: clickng.dpr
  $(DCC)

Resource_kompilieren.bat: Resource_kompilieren.bat
  call $**


